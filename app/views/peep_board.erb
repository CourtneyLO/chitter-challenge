<h1> Peep Board </h1>

<ul id="peeps">
  <% @peeps.each do |peep| %>
  <li>
  <%= peep.peep %>
  </li>
  <% end %>
</ul>

<form action="/new_peep" method="get">
  <input type="submit" value="Add Peep" >
</form>

